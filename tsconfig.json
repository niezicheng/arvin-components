{
  "compilerOptions": {
    "downlevelIteration": true,
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@arvin/react-ui": ["src"],
      "@arvin/react-ui/*": ["src/*", "*"]
    },
    "resolveJsonModule": true,
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*", "jest.setup.ts"],
  "exclude": ["src/**/__tests__"]
}
